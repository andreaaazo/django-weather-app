{
    "version": 2,
    "builds": [{
        "src": "build_files.sh",
        "use": "@vercel/static-build",
        "config": {
            "distDir": "staticfiles"
        }
    }],
    "routes": [{
            "src": "/static/(.*)",
            "dest": "/static/$1"
        },
        {
            "src": "/(.*)",
            "dest": "databerry_project/wsgi.py"
        }
    ],
    "devCommand": "gunicorn databerry_project.wsgi"
}