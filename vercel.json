{
    "version": 2,
    "functions": {
        "run_server.sh": { "runtime": "vercel-bash@4.1.0" }
    },
    "builds": [{
        "src": "build_files.sh",
        "use": "@vercel/static-build",
        "config": {
            "distDir": "staticfiles"
        }
    }],
    "routes": [{
            "src": "/static/(.*)",
            "dest": "/static/$1"
        },
        {
            "src": "/(.*)",
            "dest": "http://127.0.0.1:8000/"
        }
    ]
}