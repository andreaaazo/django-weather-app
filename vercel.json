{
    "version": 2,
    "builds": [{
            "src": "run_server.sh",
            "use": "@vercel/static-build"
        },
        {
            "src": "build_files.sh",
            "use": "@vercel/static-build",
            "config": {
                "distDir": "staticfiles"
            }
        }
    ],
    "routes": [{
            "src": "/static/(.*)",
            "dest": "/static/$1"
        },
        {
            "src": "/(.*)",
            "dest": "http://127.0.0.1:8000/"
        }
    ]
}